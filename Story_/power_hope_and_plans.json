					"type": "dialogue",
					"icon_file": "atago-skin1_1",
					"name": "Atago",
					"chinese": "",
					"chineseTL": "",
					"chineseTLnote": "",
					"japanese": "",
					"japaneseTL": "",
					"japaneseTLnote": "",
